        "import re\n",
        "import requests, json, time\n",
        "import pandas as pd\n",
        "\n",
        "url = \"https://openapi.naver.com/v1/datalab/shopping/category/keywords\"\n",
        "headers = {\n",
        "    \"X-Naver-Client-Id\": \"ID\",\n",
        "    \"X-Naver-Client-Secret\": \"Secret\"\n",
        "}\n"
      ],
      "metadata": {
