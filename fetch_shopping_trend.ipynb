    {
      "cell_type": "code",
      "source": [
        "import re\n",
        "\n",
        "base_patterns = {\n",
        "    \"갤럭시\": [\"S25\", \"Z Fold 7\", \"Z Flip 7\", \"A16 5G\", \"A36 5G\", \"A56 5G\", \"M36 5G\"],\n",
        "    \"Galaxy\": [\"S25\", \"Z Fold 7\", \"Z Flip 7\", \"A16 5G\", \"A36 5G\", \"A56 5G\", \"M36 5G\"],\n",
        "    \"iPhone\": [\"16e\", \"17\", \"17 Air\", \"17 Pro\", \"17 Pro Max\"],\n",
        "    \"아이폰\": [\"16e\", \"17\", \"17 Air\", \"17 Pro\", \"17 Pro Max\"]\n",
        "}\n",
        "\n",
        "s25_suffixes = [\"\", \"+\", \" Ultra\", \" Edge\", \" FE\"]\n",
        "fold_variants = [\"Fold\", \"폴드\"]\n",
        "flip_variants = [\"Flip\", \"플립\"]\n",
        "z_variants = [\"Z\", \"z\", \"제트\"]\n",
        "iphone_air = [\"Air\", \"air\", \"에어\"]\n",
        "iphone_pro = [\"Pro\", \"pro\", \"프로\"]\n",
        "iphone_max = [\"Max\", \"max\", \"맥스\"]\n",
        "\n",
        "expanded_keywords = []\n",
        "\n",
        "for brand, models in base_patterns.items():\n",
        "    for model in models:\n",
        "        # Galaxy S25 시리즈\n",
        "        if re.match(r\"[Ss]25\", model, flags=re.IGNORECASE):\n",
        "            for suffix in s25_suffixes:\n",
        "                for space in [\"\", \" \"]:\n",
        "                    variant = f\"{brand}{space}{re.sub(r'([Ss]25)', f'S25{suffix}', model, flags=re.IGNORECASE)}\"\n",
        "                    expanded_keywords.extend([variant, variant.replace(\" \", \"\")])\n",
        "\n",
        "        # Galaxy Z 시리즈 (Fold/Flip , 한영, ignorecase)\n",
        "        elif re.search(r\"Z\\s?(Fold|Flip)\", model, flags=re.IGNORECASE):\n",
        "            for z in z_variants:\n",
        "                for f in fold_variants + flip_variants:\n",
        "                    variant = re.sub(r\"Z\\s?(Fold|Flip)\", f\"{z} {f}\", model, flags=re.IGNORECASE)\n",
        "                    for space in [\"\", \" \"]:\n",
        "                        expanded_keywords.extend([\n",
        "                            f\"{brand}{space}{variant}\",\n",
        "                            f\"{brand}{space}{variant}\".replace(\" \", \"\")\n",
        "                        ])\n",
        "\n",
        "        # iPhone 시리즈 (Air,Pro, Max)\n",
        "        elif re.search(r\"17\", model):\n",
        "            variants = []\n",
        "            if re.search(r\"Air\", model, flags=re.IGNORECASE):\n",
        "                variants = iphone_air\n",
        "            elif re.search(r\"Pro Max\", model, flags=re.IGNORECASE):\n",
        "                variants = [f\"{p} {m}\" for p in iphone_pro for m in iphone_max]\n",
        "            elif re.search(r\"Pro\", model, flags=re.IGNORECASE):\n",
        "                variants = iphone_pro\n",
        "            else:\n",
        "                variants = [\"\"]\n",
        "\n",
        "            for v in variants:\n",
        "                variant = re.sub(r\"(Air|Pro(\\s?Max)?)\", v, model, flags=re.IGNORECASE)\n",
        "                for space in [\"\", \" \"]:\n",
        "                    expanded_keywords.extend([\n",
        "                        f\"{brand}{space}{variant}\",\n",
        "                        f\"{brand}{space}{variant}\".replace(\" \", \"\")\n",
        "                    ])\n",
        "\n",
        "        # 5G optional\n",
        "        else:\n",
        "            # 5G가 붙은 모델이면 5G 없는 버전도 추가\n",
        "            model_clean = re.sub(r\"\\s?5G\", \"\", model, flags=re.IGNORECASE)\n",
        "            variants = [model, model_clean] if model != model_clean else [model]\n",
        "\n",
        "            for v in variants:\n",
        "                for space in [\"\", \" \"]:\n",
        "                    expanded_keywords.extend([\n",
        "                        f\"{brand}{space}{v}\",\n",
        "                        f\"{brand}{space}{v}\".replace(\" \", \"\")\n",
        "                    ])\n",
        "\n",
        "# 중복 제거\n",
        "expanded_keywords = sorted(set(expanded_keywords))\n",
        "print(f\"최종 확장된 키워드 목록: {expanded_keywords},\\n{len(expanded_keywords)}개\")"
      ],
